{
  "name": "timing-stats",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "bootstrap": "^3.3.7",
    "chai": "^3.5.0",
    "d3": "^4.4.0",
    "gh-pages": "^0.12.0",
    "mocha": "^3.2.0",
    "moment": "^2.17.1",
    "webpack": "2.2.0-rc.0",
    "webpack-dev-server": "2.1.0-beta.12"
  },
  "scripts": {
    "copy:bootstrap": "rm -rf dist/css && mkdir dist/css && cp -R node_modules/bootstrap/dist/css/* dist/css",
    "copy": "yarn copy:bootstrap",
    "build:js": "webpack",
    "build": "yarn build:js",
    "assets": "yarn copy && yarn build",
    "start": "yarn assets && webpack-dev-server --progress --colors --hot --content-base dist/",
    "test": "mocha",
    "deploy": "yarn assets && node ./script/deploy.js && mkdir timing_data_temp_dir && node ./script/add_timing_data.js"
  }
}
